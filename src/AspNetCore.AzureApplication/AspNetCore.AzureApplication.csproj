<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net6.0;net7.0</TargetFrameworks>
    <ImplicitUsings>disable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <InvariantGlobalization>true</InvariantGlobalization>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <RootNamespace>Microsoft.AspNetCore.Builder</RootNamespace>
    <AssemblyName>GGroupp.Infra.AspNetCore.AzureApplication</AssemblyName>
    <Version>2.3.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="GGroupp.Infra.AspNetCore.ApplicationInsights" Version="2.2.0" />
    <PackageReference Include="GGroupp.Infra.AspNetCore.EndpointApplication" Version="2.2.0" />
  </ItemGroup>

</Project>
